<h1>Danh sach nguoi dung</h1>
<form action="">
  <select name="status">
    <option value="all">tat ca</option>
    <option value="active">kich hoat</option>
    <option value="inactive">khong kich hoat</option>
  </select>
  <input type="search" placeholder="tim kiem..." name="keyword" />
  <button>tim kiem</button>
</form>
<table border="1" cellspacing="0">
  <tr>
    <th>STT</th>
    <th>Name</th>
    <th>Email</th>
    <th>Status</th>
    <th>Time</th>
    <th width="5%">Sua</th>
    <th>Xoa</th>
  </tr>

  <% users.forEach(({id,name,email,status,created_at:createdAt} , index) => { %>
  <tr>
    <td><%= index+1 %></td>
    <td><%= name %></td>
    <td><%= email %></td>
    <td><%= status %></td>
    <td><%= moment(createdAt).format('DD/MM/YYYY') %></td>
    <td><a href="">Sua</a></td>
    <td><a href="">Xoa</a></td>
  </tr>
  <% }) %>
</table>
